<%= link_to 'Atras', bookshelves_path, class: 'btn btn-outline-primary btn-lg'%>
<p><%= @author.name %> <%= @author.last_name %></p>
<% if current_user.admin %>
<p><%= link_to 'Edit', edit_author_path(@author) %> <%= link_to  "Delete", author_path(@author), method: :delete, data: {confirm: 'Are you sure?'} %> </p>
<% end %>